datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Answer {
  id        String   @id @default(uuid())
  returnId  String
  section   String
  data      Json     @default("{}")
  version   Int      @default(1)
  updatedAt DateTime @updatedAt

  @@unique([returnId, section])
}
