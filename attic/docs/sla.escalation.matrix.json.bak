{
  "version": 1,
  "levels": ["none","notify","page"],
  "rules": [
    { "when": {"overall":"fail"}, "level":"page" },
    { "when": {"overall":"warn", "streak_gte":3}, "level":"notify" },
    { "when": {"d7_status":"fail"}, "level":"notify" },
    { "when": {"d7_rate_gte":10}, "level":"notify" },
    { "when": {"streak_gte":5}, "level":"page" }
  ]
}
